-- Dominator Library
-- Baseado em Fluxus UI e Orion UI
-- Criado para facilitar a criacao de GUIs e utilidades

local Dominator = {}

-- Carregar Fluxus UI
local Fluxus = loadstring(game:HttpGet("https://raw.githubusercontent.com/Imalwibest/Imalwibest/refs/heads/main/Fluxus%20Ui.lib"))()
-- Carregar Orion UI
local Orion = loadstring(game:HttpGet("https://raw.githubusercontent.com/jensonhirst/Orion/main/source"))()

-- Criar uma GUI usando Orion UI
function Dominator.CreateOrionGUI()
    local Window = Orion:MakeWindow({Name = "Dominator Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "Dominator"})
    local Tab = Window:MakeTab({Name = "Main", Icon = "rbxassetid://4483345998", PremiumOnly = false})
    
    Tab:AddButton({Name = "Exemplo de Botao", Callback = function()
        print("Botao pressionado!")
    end})
    
    Orion:Init()
end

-- Criar uma GUI usando Fluxus UI
function Dominator.CreateFluxusGUI()
    local Window = Fluxus:CreateWindow("Dominator Hub")
    local Tab = Window:CreateTab("Main")
    
    Tab:CreateButton("Exemplo de Botao", function()
        print("Botao Fluxus pressionado!")
    end)
end

-- Funcoes utilitarias
function Dominator.TeleportTo(position)
    local player = game.Players.LocalPlayer
    if player and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
        player.Character.HumanoidRootPart.CFrame = CFrame.new(position)
    end
end

function Dominator.SetWalkSpeed(speed)
    local player = game.Players.LocalPlayer
    if player and player.Character and player.Character:FindFirstChild("Humanoid") then
        player.Character.Humanoid.WalkSpeed = speed
    end
end

function Dominator.SetJumpPower(power)
    local player = game.Players.LocalPlayer
    if player and player.Character and player.Character:FindFirstChild("Humanoid") then
        player.Character.Humanoid.JumpPower = power
    end
end

return Dominator
